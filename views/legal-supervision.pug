extends layout

include helpers/mixins

block content
	+banner(`${imgPath}/legal-supervision/about-accreditation-herobanner.jpg`, '监管资质')
	section.section.section--gray.supervision
		div.container
			+styledHeading('法律监管资质', true)
			div.row.supervision__list
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=src=`${imgPath}/partner/logo_asic.jpg` alt="asic's logo")
						div.supervision__content
							div.supervision__title Australian Securities & Investment Commission (ASIC)
							div.supervision__des 澳大利亚证券及投资监察委员会颁发金融服务牌照（编号：334293）
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_companies-office.jpg` alt="companies-office's logo")
						div.supervision__content
							div.supervision__title Companies Office – Financial Service Provider Register
							div.supervision__des 注册金融服务商牌照（编号：FSP1782）
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_fintrac.jpg` alt="ftracc's logo")
						div.supervision__content
							div.supervision__title Financial Transactions Reports Analysis Centre of Canada
							div.supervision__des 加拿大金融交易与金融报告分析中心颁发货币服务业务注册牌照（编号：M08934674）
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_sfc.jpg` alt="sfc's logo")
						div.supervision__content
							div.supervision__title Securities and Futures Commission of Hong Kong
							div.supervision__des 杠杆式外汇交易牌照（编号：BHR931）
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_hkcustomsandexcise.jpg` alt="logo_hkcustomsandexcise's logo")
						div.supervision__content
							div.supervision__title Money Service Operator License of Hong Kong Customs and Excise Department
							div.supervision__des 香港海关总署注册金钱服务经营者牌照
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_worldwidemap.jpg` alt="worldwidemap's logo")
						div.supervision__content
							div.supervision__title 加速全球布局
							div.supervision__des 敬请期待更多...
