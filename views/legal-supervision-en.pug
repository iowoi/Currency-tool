extends layout-en

include helpers/mixins
include helpers/mixins-en

block content
	+banner(`${imgPath}/legal-supervision/about-accreditation-herobanner.jpg`, 'ACCREDITATION ')
	section.section.section--gray.supervision
		div.container
			+styledHeading('Accreditation', true)
			div.row.supervision__list
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=src=`${imgPath}/partner/logo_asic.jpg` alt="asic's logo")
						div.supervision__content
							div.supervision__title Australian Securities & Investment Commission (ASIC)
							div.supervision__des Australian Financial Services (AFS) License (No: 334293)
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_companies-office.jpg` alt="companies-office's logo")
						div.supervision__content
							div.supervision__title Companies Office â€“ Financial Service Provider Register
							div.supervision__des Financial Service Provider Register (FSPR) (No: FSP1782)
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_fintrac.jpg` alt="ftracc's logo")
						div.supervision__content
							div.supervision__title Financial Transactions Reports Analysis Centre of Canada
							div.supervision__des Money Services Business License (No: M08934674)
				div.col-md-4.col-sm-6
					div.supervision__card
						div.logo.supervision__logo
							div.logo__container
								img(src=`${imgPath}/partner/logo_sfc.jpg` alt="sfc's logo")
